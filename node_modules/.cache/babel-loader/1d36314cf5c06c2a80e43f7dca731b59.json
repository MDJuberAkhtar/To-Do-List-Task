todos.filter(x=> x.id !=id);\r\n    },\r\n    update_todo(state, todo){\r\n        let index = state.todos.findIndex(x=> x.id == todo.id);\r\n        if(index != -1){\r\n            state.todos[index]= todo;\r\n        }\r\n    }\r\n}\r\n\r\nexport default{\r\n    state,\r\n    getters,\r\n    actions,\r\n    mutations\r\n}"]}]}